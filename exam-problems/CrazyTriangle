public class CrazyTriangle {

    public static void main (String[] argv)
    {
	int N = 12;
        
        for (int i=0; i<=N; i++) { 
            for (int j=1; j<=N-i; j++) { 
                System.out.print (j + "  "); 
                if (j < 9) { 
                System.out.print (" "); 
                }
            }
            System.out.println (); 
       }

    }

}
